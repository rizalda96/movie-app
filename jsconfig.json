{
  "include": ["./vite.config.*", "./src/**/*", "./src/**/*.vue", "./typed-router.d.ts"],
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@images/*": ["./src/assets/images/*"],
      "@styles/*": ["./src/assets/styles/*"],
      "@components/*": ["./src/components/*"],
      "@views/*": ["./src/views/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
